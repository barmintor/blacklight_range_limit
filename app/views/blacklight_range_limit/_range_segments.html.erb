<% # must pass in local variable :solr_field
%>

<ul class="facet-values list-unstyled">
  <% solr_range_queries_to_a(solr_field).each do |hash| %>
    <%- item_presenter = facet_item_presenter(facet_configuration_for_field(solr_field), OpenStruct.new(hash), solr_field) %>
    <%= render Blacklight::FacetItemComponent.new(facet_item: item_presenter) %>
  <% end %>
</ul>
